! to match gnome-terminal "Linux console" scheme
! foreground/background
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/e800

URxvt*foreground: #ffffff
! black
URxvt.color0  : rgba:0000/0000/0000/e800
URxvt.color8  : #555555
! red
URxvt.color1  : #AA0000
URxvt.color9  : #FF5555
! green
URxvt.color2  : #00AA00
URxvt.color10 : #55FF55
! yellow
URxvt.color3  : #AA5500
URxvt.color11 : #FFFF55
! blue
URxvt.color4  : #0000BB
URxvt.color12 : #9999FF
! magenta
URxvt.color5  : #AA00AA
URxvt.color13 : #FF55FF
! cyan
URxvt.color6  : #00AAAA
URxvt.color14 : #55FFFF
! white
URxvt.color7  : #AAAAAA
URxvt.color15 : #FFFFFF

URxvt.scrollBar : false

URxvt*font: xft:Monospace:pixelsize=11
URxvt*boldFont: xft:Monospace:pixelsize=11

URxvt*urgentOnBell : true
URxvt*secondary-wheel : true

URxvt*termName:xterm
